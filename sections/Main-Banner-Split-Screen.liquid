{% style %}
    .block-image-container {
      display: flex;
      gap: 20px;
    }
    .qq-block-image-text {
          margin-top: 10px;
          margin-bottom: 10px;
          align-self: center;
          max-width: 40%;
          color: white;
    }
    .qq-block-image-text h2 {
      color: white;
    }

    .qq-section-wrap-fullwidth, .qq-section-wrap-regular {
      position: relative;
    }

    .qq-overlay-image-container {
      position: absolute !important;
    }

    /* Video Overlay Styles */
.qq-section-video {
  position: relative;
  overflow: hidden; /* This will contain the overlay within the video section */
}

.qq-video-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 9;
  pointer-events: none;
  overflow: hidden; /* Ensure nothing extends outside */
}

    .qq-video-overlay .overlay-background {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
    }

    .qq-video-overlay .circle-cutout {
      position: absolute;
      border-radius: 50%;
      background-color: transparent;
      box-shadow: 0 0 0 9999px var(--overlay-color); /* Creates cutout effect */
      pointer-events: none;
    }

    @media only screen and (max-width: 767px) {
    .qq-video-overlay .circle-cutout {
      width: {{videoMobileCutoutSize}}px !important;
      height: {{videoMobileCutoutSize}}px !important;
    }
  }

    /* Mobile overlay image adjustments */
    {% if overlayImageEnabled and overlayImage != blank and overlayImageMobileSettings %}
    #{{sectionID}} .qq-overlay-image-container {
      width: {{overlayImageMobileWidth}}% !important;
      top: {{overlayImageMobileTopPosition}}% !important;
      left: {{overlayImageMobileLeftPosition}}% !important;
      right: auto !important;
    }    
    {% endif %}
    
  {% endstyle %}
  
