        {% capture imageElement %}
          {% if sectionImage == true %}
            {% if mainImage != blank %}
              {% assign loopImage = mainImage %}								
              {% assign mediaSize = mainMediaSize %}								
              {% liquid 
                      if loopImage.alt != blank
                        assign loopImageAlt = 'alt="' | append: loopImage.alt | append: '"'
                      else
                        assign loopImageAlt = ''
                      endif
                      assign proportion = loopImage.aspect_ratio
                      assign imageW = loopImage.width
                      assign imageH = loopImage.height   
                      for imageSize in imageSizes
                        assign maxWidthAllowed = imageSize | remove: 'x'
                        assign maxHeightAllowed = imageSize | remove: 'x'
                        if proportion > 1
                          # landscape
                          assign mostW = maxWidthAllowed
                          assign mostH = mostW | times: proportion
                        elsif proportion < 1
                          # portrait
                          assign mostH = maxHeightAllowed
                          assign mostW = mostH | times: proportion
                        else
                          # square
                          assign mostW = maxWidthAllowed
                          assign mostH = mostW
                        endif
                        assign iw = imageW | at_most: mostW | round
                        assign ih	= imageH | at_most: mostH | round
                        if mediaSize == 'portrait'
                          if halfSquarePortrait == true  
                            assign ih = iw | times: 2 | round
                              if imageH < ih
                                  assign ih = imageH
                                  assign iw = ih | divided_by: 2 | round
                              endif
                              if imageW < iw
                                  assign iw = imageW
                                  assign ih = iw | times: 2 | round
                              endif
                          else 
                            assign ih = iw | divided_by: 0.6666667 | round
                            if imageH < ih
                              assign ih = imageH
                              assign iw = ih | divided_by: 1.5 | round
                            endif
                            if imageW < iw
                              assign iw = imageW
                              assign ih = iw | divided_by: 0.666667 | round
                            endif
                          endif
                        elsif mediaSize == 'vertical'
                          assign ih = iw | times: 16 | divided_by: 9 | round
                          if imageH < ih
                            assign ih = imageH
                            assign iw = ih | times: 9 | divided_by: 16 | round
                          endif
                          if imageW < iw
                            assign iw = imageW
                            assign ih = iw | times: 16 | divided_by: 9 | round
                          endif     
                        elsif mediaSize == 'horizontal'
                          assign ih = iw | times: 9 | divided_by: 16 | round
                          if imageH < ih
                            assign ih = imageH
                            assign iw = ih | times: 16 | divided_by: 9 | round
                          endif
                          if imageW < iw
                            assign iw = imageW
                            assign ih = iw | times: 9 | divided_by: 16 | round
                          endif                                          
                        elsif mediaSize == 'square'
                          assign ih = iw
                          if iw > imageW
                            assign iw = imageW
                            assign ih = iw
                          endif
                          if ih > imageH
                            assign ih = imageH
                            assign iw = ih
                          endif
                        elsif mediaSize == 'landscape'
                          if halfSquareLandscape == true 
                            assign ih = iw | times: 0.5 | round
                            if imageH < ih
                              assign ih = imageH
                              assign iw = ih | times: 2 | round
                            endif
                            if imageW < iw
                              assign iw = imageW
                              assign ih = iw | times: 0.5 | round
                            endif                        
                          else 
                           assign ih = iw | times: 4 | divided_by: 6 | round
                            if imageH < ih
                              assign ih = imageH
                              assign iw = ih | times: 6 | divided_by: 4 | round
                            endif
                            if imageW < iw
                              assign iw = imageW
                              assign ih = iw | times: 4 | divided_by: 6 | round
                            endif
                          endif
                        elsif mediaSize == 'original'
                          assign ih = iw | divided_by: proportion | round
                        endif
                        assign size = iw | append: 'x' | append: ih
                        assign imageUrl = loopImage | img_url: size, crop: 'center'
                        if forloop.first
                          assign imageUrls = imageUrl
                        else
                          assign imageUrls = imageUrls | append: ',' | append: imageUrl
                        endif                      
                      endfor
                      assign imageUrls = imageUrls | split: ','                                                              
  %}
