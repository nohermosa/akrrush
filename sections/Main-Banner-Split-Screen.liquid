  {% schema %}
  {
    "name": "Main Banner Split",
    "tag": "section",
    "class": "qq-splitscreen-section qq-splitscreen-with-video",
    "settings": [
      {
        "type": "text",
        "id": "section_class",
        "label": "Section Class",
        "info": "Add section class (without .). Use spaces to add multiple classes"
      },
      {
  "type": "header",
  "content": "Overlay Image Settings"
},
{
  "type": "checkbox",
  "id": "enable_overlay_image",
  "label": "Enable Overlay Image",
  "default": false,
  "info": "Add an image that can be positioned anywhere on top of the section"
},
{
  "type": "image_picker",
  "id": "overlay_image",
  "label": "Overlay Image",
  "info": "Choose an image to overlay"
},
{
  "type": "range",
  "id": "overlay_image_width",
  "min": 5,
  "max": 100,
  "step": 1,
  "unit": "%",
  "label": "Image Width",
  "default": 20,
  "info": "Width of the overlay image relative to the section"
},
{
  "type": "range",
  "id": "overlay_image_top_position",
  "min": -50,
  "max": 150,
  "step": 5,
  "unit": "%",
  "label": "Top Position",
  "default": 10,
  "info": "Position from the top of the section"
},
{
  "type": "range",
  "id": "overlay_image_left_position",
  "min": -50,
  "max": 150,
  "step": 5,
  "unit": "%",
  "label": "Left Position",
  "default": 10,
  "info": "Position from the left of the section"
},
{
  "type": "range",
  "id": "overlay_image_right_position",
  "min": -50,
  "max": 150,
  "step": 5,
  "unit": "%",
  "label": "Right Position",
  "default": 10,
  "info": "Position from the right of the section. Left takes precedence if both are set"
},
{
  "type": "range",
  "id": "overlay_image_rotation",
  "min": -180,
  "max": 180,
  "step": 5,
  "unit": "°",
  "label": "Rotation",
  "default": 0,
  "info": "Rotate the image (degrees)"
},
{
  "type": "range",
  "id": "overlay_image_opacity",
  "min": 10,
  "max": 100,
