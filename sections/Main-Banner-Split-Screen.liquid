                          assign ih = iw | times: 9 | divided_by: 16 | round
                          if imageH < ih
                            assign ih = imageH
                            assign iw = ih | times: 16 | divided_by: 9 | round
                          endif
                          if imageW < iw
                            assign iw = imageW
                            assign ih = iw | times: 9 | divided_by: 16 | round
                          endif                                          
                        elsif mediaSize == 'square'
                          assign ih = iw
                          if iw > imageW
                            assign iw = imageW
                            assign ih = iw
                          endif
                          if ih > imageH
                            assign ih = imageH
