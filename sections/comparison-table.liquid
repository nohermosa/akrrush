    {% capture columnHeaders %}
      <div class="qq-comparison-row qq-header">
        {% if hideFeaturesColumn == false %}
          <div class="qq-ct--cell"></div> {% comment %} kept empty to retain grid structure for comparison table{% endcomment %}
        {% endif %}
        {%- for i in (1..columnsAmount) -%}
          {% liquid
            assign _title_tmp = 'column_header_title_' | append: i
            assign _text_tmp = 'column_header_text_' | append: i
            assign _image_tmp = 'column_header_image_' | append: i
            assign cell_title = section.settings[_title_tmp]
            assign cell_text = section.settings[_text_tmp]
            assign cell_image = section.settings[_image_tmp]
            if cell_image.alt != blank
              assign cellImageAlt = 'alt="' | append: cell_image.alt | append: '"'
            else
              assign cellImageAlt = ''
            endif
          %}
          {% unless cell_title == blank and cell_image == blank %}
              <div class="qq-ct--cell">
                {% if cell_image != blank %}
                  <img src="{{ cell_image | img_url: 'master' }}" {{cellImageAlt}} loading="lazy" style="max-width: 100%; height: auto;">
                {% else %}
                  <div class="qq-image-placeholder"></div>
                {% endif %}
                  {% if cell_title != blank %}
                    <h3 class="qq-ct--title">{{cell_title}}</h3>
                  {% endif %}
                  {% if cell_text != blank %}
                    <p>{{cell_text}}</p>
                  {% endif %}
              </div>
          {% endunless %}
        {% endfor %}
      </div>
    {% endcapture %}
    <div class="qq-comparison-table {{sectionClass}}" id="{{divID}}">
        <div class="qq-comparison-table-inner">
            {% if sectionTitle != blank %}
              <div class="qq-comparison-table-title">{{ sectionTitle }}</div>
            {% endif %}
            {% if mobileNavigation == 'scroll' and columnTitleSticky == true %}
              <div class="qq-mobile-scroll-wrapper for-sticky syncscroll" name="myElements">
                <div class="qq-wrapper">
                  {{columnHeaders}}
                </div>
              </div>
            {% endif %}
            {% if mobileNavigation == 'scroll' %}
              <div class="qq-mobile-scroll-wrapper syncscroll" name="myElements">
            {% endif %}
            <div class="qq-comparison-columns">
              {{columnHeaders}}          
              {%- for block in section.blocks -%}
                <div class="qq-comparison-row">
                  {% if hideFeaturesColumn == false %}
                    <div class="qq-ct--cell qq-row-title">
                      <div class="{% if block.settings.row_image  %}img-{{ rowTitleImgPlacement }} {% if mobileNavigation == 'scroll' %}img-{{rowTitleImgPlacementMobile}}-mobile{% endif %}{% endif %}">
                        {% if block.settings.row_image  %}
                          <img src="{{block.settings.row_image | img_url: '150x150', crop: 'center' }}" alt="{{block.settings.row_image.alt}}" loading="lazy">
                        {% endif %}
                        {% if block.settings.title  %}
                          <h4>{{block.settings.title }}</h4>
                        {% endif %}
                      </div>
                    </div>
                  {% endif %}
                  {%- for i in (1..columns) -%}
                    {% if i <= columnsAmount %}
                      {% liquid
                        assign text_tmp = 'column_' | append: i
                        assign cell_text = block.settings[text_tmp]
                        case i
                          when 1
                            assign icon_tmp = 'column_icon_one'
                          when 2
                            assign icon_tmp = 'column_icon_two'
                          when 3
                            assign icon_tmp = 'column_icon_three'
                          when 4
                            assign icon_tmp = 'column_icon_four'
                          when 5
                            assign icon_tmp = 'column_icon_five'
                        endcase
                        assign cellIcon = block.settings[icon_tmp]
                        assign cellIconUrl = cellIcon | img_url: '150x150', crop: 'center'
                        if cellIcon.alt != blank
                          assign cellIconAlt = 'alt="' | append: cellIcon.alt | append: '"'
                        else
                          assign cellIconAlt = ''
                        endif
                      %}
                      <div class="qq-ct--cell">
                        {%- unless cell_text == blank and use_cell_icon == false -%}
                          <div class="qq-ct--cell-content">
                            {% if cellIcon != blank %}
                              <img src="{{ cellIconUrl }}" {{cellIconAlt}} loading="lazy">
                            {% endif %}
                            {% if cell_text != blank %}
                              {{cell_text}}
                            {% endif %}
                          </div>
                        {%- endunless -%}
                      </div>
                    {% endif %}
                  {% endfor %}
                </div>
                {% if forloop.last %}
                  {% assign hasButton = false %}
                  {%- for i in (1..columns) -%}
                      {% if i <= columnsAmount %}
                        {%- assign _button_label_tmp = 'column_button_label_' | append: i -%}
                        {%- assign _button_url_tmp = 'column_button_url_' | append: i -%}
                        {%- assign column_btn_label = section.settings[_button_label_tmp] -%}
                        {%- assign column_btn_url = section.settings[_button_url_tmp] -%}
                        {% if column_btn_label != blank and column_btn_url != blank %}
                          {% assign hasButton = true %}
                          {% break %}
                        {% endif %}
                      {% endif %}
                  {%- endfor -%}
                  {% if hasButton == true %}
                    <div class="qq-comparison-row qq-button">
                      {% if hideFeaturesColumn == false %}
                        <div class="empty">&nbsp;</div>
                      {% endif %}
                      {%- for i in (1..columns) -%}
                        {% if i <= columnsAmount %}
                          {%- assign _button_label_tmp = 'column_button_label_' | append: i -%}
                          {%- assign _button_url_tmp = 'column_button_url_' | append: i -%}
                          {%- assign column_btn_label = section.settings[_button_label_tmp] -%}
                          {%- assign column_btn_url = section.settings[_button_url_tmp] -%}
                          <div class="qq-ct--cell qq-btn-cell">
                            {% if column_btn_label != blank and column_btn_url != blank %}
                              <a href="{{column_btn_url}}" class="qq-column-btn">{{column_btn_label}}</a>
                            {% endif %}
                          </div>
                        {%- endif -%}
                      {%- endfor -%}
                    </div>
                  {% endif %}
                {% endif %}
              {% endfor %}
            </div>
            {% if mobileNavigation == 'scroll' %}
              </div>
            {% endif %}
            {% if mobileNavigation == 'slider' %}
              <div class="qq-comparison-columns-mobile">
                  <div class="qq-arrow-prev {{navigationType}}" data-top="{{columnCellPaddingVertical}}">
                    {% if navigationIconUrl != blank %}
                      <img src="{{navigationIconUrl}}" alt="Previous Navigation Arrow" loading="lazy">
                    {% else %}
                      <svg width="18" height="10" xmlns="http://www.w3.org/2000/svg"><title>Show Answer</title><g fill-rule="nonzero" stroke="#4A4B47" fill="none" stroke-linecap="square"><path d="m.834.5 3.666 4M8.5.834 4.5 4.5"/></g></svg>
                    {% endif %}
                  </div>
                  <div class="qq-arrow-next {{navigationType}}" data-top="{{columnCellPaddingVertical}}">
                    {% if navigationIconUrl != blank %}
                      <img src="{{navigationIconUrl}}" alt="Next Navigation Arrow" loading="lazy">
                    {% else %}
                      <svg width="18" height="10" xmlns="http://www.w3.org/2000/svg"><title>Show Answer</title><g fill-rule="nonzero" stroke="#4A4B47" fill="none" stroke-linecap="square"><path d="m.834.5 3.666 4M8.5.834 4.5 4.5"/></g></svg>
                    {% endif %}
                  </div>                            
                  <div class="qq-comparison-table-slider"
                  data-navigation="{{mobileNavigation}}"
                  data-mobileColumns="{{mobileColumns}}"
                  data-mobileColumnsMargin={{mobileColumnsMargin}}>
                    {%- for i in (1..columnsAmount) -%}
                      <div class="qq-comparison-column">
                          {%- assign _title_tmp = 'column_header_title_' | append: i -%}
                          {%- assign _text_tmp = 'column_header_text_' | append: i -%}
                          {%- assign _image_tmp = 'column_header_image_' | append: i -%}
                          {%- assign cell_title = section.settings[_title_tmp] -%}
                          {%- assign cell_text = section.settings[_text_tmp] -%}
                          {%- assign cell_image = section.settings[_image_tmp] -%}
                          {% unless cell_title == blank and cell_image == blank %}
                              <div class="qq-ct--cell qq-header-cell">
                                  {% if cell_image != blank %}
                                  {% liquid
                                    if cell_image.alt != blank
                                      assign cellImageAlt = 'alt="' | append: cell_image.alt | append: '"'
                                    else
                                      assign cellImageAlt = ''
                                    endif
                                  %}
                                  <img src="{{ cell_image | img_url: 'master' }}" {{cellImageAlt}} loading="lazy" style="max-width: 100%; height: auto;">
                                  {% endif %}
                                  {% if cell_title != blank %}
                                    <h4>{{cell_title}}</h4>
                                  {% endif %}
                                  {% if cell_text != blank %}
                                    <p>{{cell_text}}</p>
                                  {% endif %}
                              </div>
                          {% endunless %}
                          {%- for block in section.blocks -%}
                              {% liquid
                                assign text_tmp = 'column_' | append: i
                                assign cell_text = block.settings[text_tmp]
                                comment
                                  assign icon_tmp = 'column_icon_' | append: i
                                endcomment 
                                case i
                                  when 1
                                    assign icon_tmp = 'column_icon_one'
                                  when 2
                                    assign icon_tmp = 'column_icon_two'
                                  when 3
                                    assign icon_tmp = 'column_icon_three'
                                  when 4
                                    assign icon_tmp = 'column_icon_four'
                                  when 5
                                    assign icon_tmp = 'column_icon_five'
                                endcase
                                assign cellIcon = block.settings[icon_tmp]
                                assign cellIconUrl = cellIcon | img_url: '150x150', crop: 'center'
                                if cellIcon.alt != blank
                                  assign cellIconAlt = 'alt="' | append: cellIcon.alt | append: '"'
                                else
                                  assign cellIconAlt = ''
                                endif
                              %}
                              <div class="qq-ct--cell">
                                {% if hideFeaturesColumn == false %}
                                  <div class="qq-ct--title">
                                    <div class="qq-mobile-row-title{% if block.settings.row_image %} img-{{ rowTitleImgPlacementMobile }}{% endif %}">
                                      {% if block.settings.row_image  %}
                                        <img src="{{block.settings.row_image | img_url: '150x150', crop: 'center' }}" alt="{{block.settings.row_image.alt}}" loading="lazy">
                                      {% endif %}
                                      {% if block.settings.title  %}
                                        <h4>{{block.settings.title }}</h4>
                                      {% endif %}
                                    </div>
                                  </div>
                                {% endif %}
                                <div class="qq-ct--cell-content">
                                  {% if cellIcon %}
                                    <img src="{{ cellIconUrl }}" {{cellIconAlt}} loading="lazy">
                                  {% endif %}
                                  {% if cell_text != blank %}
                                    {{cell_text}}
                                  {% endif %}
                                </div>
                              </div>
                              {% if forloop.last %}
                                {%- assign _button_label_tmp = 'column_button_label_' | append: i -%}
                                {%- assign _button_url_tmp = 'column_button_url_' | append: i -%}
                                {%- assign column_btn_label = section.settings[_button_label_tmp] -%}
                                {%- assign column_btn_url = section.settings[_button_url_tmp] -%}
                                {% if column_btn_label != blank and column_btn_url != blank %}
                                  <div class="qq-ct--cell qq-btn-cell">
                                    <a href="{{column_btn_url}}" class="qq-column-btn">{{column_btn_label}}</a>
                                  </div>
                                {% endif %}
                              {% endif %}
                          {% endfor %}
                      </div>
                    {% endfor %}
                  </div>
              </div>
            {% endif %}
            {% if textBelowTable != blank %}
              <div class="qq-text-below-table">{{textBelowTable}}</div>
            {% endif %}
        </div>
    </div>
    <style>
      

      .qq-header-cell {
        min-height: 100px;
        display: flex;
        flex-direction: column;
        justify-content: center;
