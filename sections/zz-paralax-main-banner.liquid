        assign enableOverlayImage = section.settings.enable_overlay_image
        assign overlayImage = section.settings.overlay_image
        assign overlayImageSize = section.settings.overlay_image_size
        assign overlayImagePositionX = section.settings.overlay_image_position_x
        assign overlayImagePositionY = section.settings.overlay_image_position_y
        assign overlayImageZIndex = section.settings.overlay_image_z_index
        assign overlayImageOpacity = section.settings.overlay_image_opacity
        
