    <div class="{{sectionClass}}" id="{{divID}}">
<!-- Overlay -->   
        {% if enableOverlayImage and overlayImage != blank %}
            <div class="overlay-image-container"></div>
            {% endif %}
<!-- End Overlay -->           
      <div class="qq-parallax-wrapper {{wrapperClass}}" data-disable-mobile="{{parallaxDisableMobile}}">
        {% if bannerVideo != blank %}
            <div class="qq-parallax-video jarallax" data-speed="{{parallaxSpeed}}" data-videosrc="{% if bannerVideo contains 'vimeo' or bannerVideo contains 'youtube' or bannerVideo contains 'youtu.be' %}{%else%}mp4:{% endif %}{{bannerVideo}}">
                {% comment %} {% if parallaxType == 'css'  %}
                    <video class="" playsinline="" muted="" onplaying="this.controls=false" autoplay="" loop="" poster="{{imageUrl}}">
                        <source src="{{bannerVideo}}" type="video/mp4; codecs=&quot;avc1.42E01E, mp4a.40.2&quot;">
                    </video>
                {% endif %} {% endcomment %}
            </div>
        {% else %}
            <div class="qq-parallax-image jarallax"  data-speed="{{parallaxSpeed}}" data-banner-image="{{imageUrl}}" data-mobile-image="{{imageUrlMobile}}">
                {% if bannerImage != blank %}
                    <img src="{{imageUrl}}" {{bannerImageAlt}} class="jarallax-img" loading="lazy">
                {% endif %}
            </div>
            {% if parallaxDisableMobile == true %}
                <img class="qq-mobile-fixed" 
                    src="{% if imageUrlMobile != blank %}{{imageUrlMobile}}{%else%}{{imageUrl}}{%endif%}" {% if imageUrlMobile != blank %}{{bannerImageMobileAlt}}{% else %}{{bannerImageAlt}}{% endif%}>
            {% endif %}
        {% endif %}
        <div class="qq-parallax-content">
            <div class="qq-parallax-content-wrapper">
                {% if title != blank %}
                    <div class="qq-title">{{title}}</div>
                {% endif %}
                {% if subtitle != blank %}
                    <div class="qq-subtitle">{{subtitle}}</div>
                {% endif %}
                {% if buttonUrl_1 != blank %}
                    <div class="qq-button-wrapper">
                        {% if buttonUrl_1 != blank %}
                            <a class="qq-btn" href="{{buttonUrl_1}}" {{buttonTab_1}}>{{buttonLabel_1}}</a>
                        {% endif %}
                    </div>
                {% endif %}
            </div>
        </div>
      </div>
    </div>
