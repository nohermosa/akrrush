        assign titleCustomFont = section.settings.title_custom_font
        assign titleFont = section.settings.title_font
        assign titleFontName = section.settings.title_font_name
        assign titleFontWeight = section.settings.title_font_weight
        assign titleColor = section.settings.title_color
        assign titleFontSize = section.settings.title_font_size
        assign titleFontSizeMobile = section.settings.title_font_size_mobile
        assign titleLineHeight = section.settings.title_line_height
        assign titleLineHeightMobile = section.settings.title_line_height_mobile
        assign titleLetterSpacing = section.settings.title_letter_spacing
        assign titleLetterSpacingMobile = section.settings.title_letter_spacing_mobile
        assign titleMarginTop = section.settings.title_margin_top
        assign titleMarginTopMobile = section.settings.title_margin_top_mobile
        assign titleMarginBottom = section.settings.title_margin_bottom
        assign titleMarginBottomMobile = section.settings.title_margin_bottom_mobile
        assign subtitle = section.settings.subtitle 	
        assign subtitleColor = section.settings.subtitle_color
        assign subtitleCustomFont = section.settings.subtitle_custom_font
        assign subtitleFont = section.settings.subtitle_font
        assign subtitleFontName = section.settings.subtitle_font_name
        assign subtitleFontWeight = section.settings.subtitle_font_weight
        assign subtitleMarginTop = section.settings.subtitle_margin_top
        assign subtitleMarginTopMobile = section.settings.subtitle_margin_top_mobile
        assign subtitleMarginBottom = section.settings.subtitle_margin_bottom
        assign subtitleMarginBottomMobile = section.settings.subtitle_margin_bottom_mobile
        assign subtitleFontSize = section.settings.subtitle_font_size
        assign subtitleFontSizeMobile = section.settings.subtitle_font_size_mobile
        assign subtitleLineHeight = section.settings.subtitle_line_height
        assign subtitleLineHeightMobile = section.settings.subtitle_line_height_mobile
        assign subtitleLetterSpacing = section.settings.subtitle_letter_spacing
        assign subtitleLetterSpacingMobile = section.settings.subtitle_letter_spacing_mobile
        assign buttonLabel_1 = section.settings.button_label_1
        assign buttonUrl_1 = section.settings.button_url_1
        assign buttonTab_1 = section.settings.button_tab_1
        if buttonTab_1 == true
            assign buttonTab_1 = 'target="blank"'
        else
            assign buttonTab_1 = ''
        endif
        assign buttonCustomFont = section.settings.button_custom_font
        assign buttonFont = section.settings.button_font
        assign buttonFontName = section.settings.button_font_name
        assign buttonFontWeight = section.settings.button_font_weight
        assign buttonMaxWidth = section.settings.button_max_width
        assign buttonFontSize = section.settings.button_font_size
        assign buttonFontSizeMobile = section.settings.button_font_size_mobile
        assign buttonPaddingHorizontal = section.settings.button_padding_horizontal
        assign buttonPaddingVertical =  section.settings.button_padding_vertical
        assign buttonBorderWidth = section.settings.button_border_width
        assign buttonRadius = section.settings.button_radius
        assign buttonBackground = section.settings.button_background_color
        assign buttonBackgroundHover = section.settings.button_background_color_hover
        assign buttonColor = section.settings.button_text_color
        assign buttonColorHover = section.settings.button_text_color_hover
        assign buttonBorderColor = section.settings.button_border_color
        assign buttonBorderColorHover = section.settings.button_border_color_hover
        assign buttonLetterSpacing = section.settings.button_letter_spacing
        assign buttonLetterSpacingMobile = section.settings.button_letter_spacing_mobile
        assign buttonMarginTop = section.settings.button_margin_top
        assign buttonMarginTopMobile = section.settings.button_margin_top_mobile
        assign buttonMarginBottom = section.settings.button_margin_bottom
        assign buttonMarginBottomMobile = section.settings.button_margin_bottom_mobile
