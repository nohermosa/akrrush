        assign sectionMarginTop = section.settings.section_margin_top
        assign sectionMarginTopMobile = section.settings.section_margin_top_mobile
        assign sectionMarginBottom = section.settings.section_margin_bottom
        assign sectionMarginBottomMobile = section.settings.section_margin_bottom_mobile
        assign sectionHeight = section.settings.section_height
        assign sectionHeightMobile = section.settings.section_height_mobile
        assign sectionPaddingHorizontal = section.settings.section_padding_horizontal
        assign sectionPaddingHorizontalMobile = section.settings.section_padding_horizontal_mobile
        assign sectionPaddingTop = section.settings.section_padding_top
        assign sectionPaddingTopMobile = section.settings.section_padding_top_mobile
        assign sectionPaddingBottom = section.settings.section_padding_bottom
        assign sectionPaddingBottomMobile = section.settings.section_padding_bottom_mobile
        assign sectionTextAlignment = section.settings.text_alignment
        assign sectionTextAlignmentMobile = section.settings.text_alignment_mobile
        assign sectionTextMaxWidth = section.settings.text_max_width
        assign blockContentAlignmentHorizontal = section.settings.content_alignment_horizontal
        assign blockContentAlignmentVertical = section.settings.content_alignment_vertical
        assign blockContentBackgroundColor = section.settings.content_background_color
        assign blockContentBackgroundColorOpacity = section.settings.content_background_color_opacity
        assign blockContentBorderRadius = section.settings.content_border_radius
        assign blockContentPaddingTop = section.settings.content_padding_top
        assign blockContentPaddingTopMobile = section.settings.content_padding_top_mobile
        assign blockContentPaddingBottom = section.settings.content_padding_bottom
        assign blockContentPaddingBottomMobile = section.settings.content_padding_bottom_mobile
        assign blockContentPaddingHorizontal = section.settings.content_padding_horizontal
        assign blockContentPaddingHorizontalMobile = section.settings.content_padding_horizontal_mobile
        assign bannerImage = section.settings.banner_image
        assign imageUrl = bannerImage | img_url: '1920x'
        if bannerImage.alt != blank 
            assign bannerImageAlt = 'alt="' | append: bannerImage.alt | append: '"'
        else 
            assign bannerImageAlt = ''
        endif
        assign bannerImageMobile = section.settings.banner_image_mobile
        if bannerImageMobile != blank
            assign imageUrlMobile = bannerImageMobile | img_url: '768x'
        else
            assign imageUrlMobile = ''
        endif
        if bannerImageMobile.alt != blank 
            assign bannerImageMobileAlt = 'alt="' | append: bannerImageMobile.alt | append: '"'
        else 
            assign bannerImageMobileAlt = ''
        endif
        assign bannerVideo = section.settings.banner_video
        assign overlayColor = section.settings.overlay_color
        assign overlayOpacity = section.settings.overlay_opacity
        assign parallaxType = section.settings.parallax_type
        assign parallaxDisableMobile = section.settings.parallax_disable_mobile
        assign parallaxSpeed = section.settings.parallax_speed
        assign parallaxDirection = section.settings.parallax_direction
        if parallaxDirection == '-'
            assign parallaxSpeed = '-' | append: parallaxSpeed
        endif
            if bannerVideo != blank
                assign parallaxSpeed = parallaxSpeed | times: 1.2
            endif
