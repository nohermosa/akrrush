    {% if enableOverlayImage and overlayImage != blank %}
  /* Create the overlay image container */
  #{{sectionID}} .overlay-image-container {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: visible;
    pointer-events: none;
    z-index: {{overlayImageZIndex}};
  }
  
  /* Create the overlay image for desktop */
  @media only screen and (min-width: 799px) {
    #{{sectionID}} .overlay-image-container:after {
      content: '';
      position: absolute;
      background-image: url({{ overlayImage | img_url: 'master' }});
      background-repeat: no-repeat;
      background-size: contain;
      max-width: {{overlayImageSize}}px;
      max-height: {{overlayImageSize}}px;
      width: {{overlayImageSize}}px;
      height: {{overlayImageSize}}px;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%) translate({{overlayImagePositionX}}%, {{overlayImagePositionY}}%);
      opacity: {{overlayImageOpacity}};
      pointer-events: none;
    }
  }
  
  /* Create the overlay image for mobile */
  @media only screen and (max-width: 798px) {
    #{{sectionID}} .overlay-image-container:after {
      content: '';
      position: absolute;
      background-image: url({% if enableOverlayImageMobile and overlayImageMobile != blank %}{{ overlayImageMobile | img_url: 'master' }}{% else %}{{ overlayImage | img_url: 'master' }}{% endif %});
      background-repeat: no-repeat;
      background-size: contain;
      max-width: {% if enableOverlayImageMobile %}{{overlayImageSizeMobile}}{% else %}{{overlayImageSize}}{% endif %}px;
      max-height: {% if enableOverlayImageMobile %}{{overlayImageSizeMobile}}{% else %}{{overlayImageSize}}{% endif %}px;
      width: {% if enableOverlayImageMobile %}{{overlayImageSizeMobile}}{% else %}{{overlayImageSize}}{% endif %}px;
      height: {% if enableOverlayImageMobile %}{{overlayImageSizeMobile}}{% else %}{{overlayImageSize}}{% endif %}px;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%) translate(
        {% if enableOverlayImageMobile %}{{overlayImagePositionXMobile}}{% else %}{{overlayImagePositionX}}{% endif %}%, 
        {% if enableOverlayImageMobile %}{{overlayImagePositionYMobile}}{% else %}{{overlayImagePositionY}}{% endif %}%
      );
      opacity: {% if enableOverlayImageMobile %}{{overlayImageOpacityMobile}}{% else %}{{overlayImageOpacity}}{% endif %};
      pointer-events: none;
    }
  }
